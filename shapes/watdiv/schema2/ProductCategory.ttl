@prefix ub: <http://db.uwaterloo.ca/~galuc/wsdbm/> .
@prefix sorg: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix : <http://example.org/> .

:ProductCategory a sh:NodeShape;
   sh:targetClass ub:ProductCategory;
   sh:property [
        sh:path ub:hasGenre;
        sh:qualifiedValueShape [
            sh:node :Genre
        ] ;
        sh:qualifiedMinCount 1
    ] ;
   sh:property [
     sh:path <http://ogp.me/ns#tag>;
     sh:minCount 2
   ] ;
   sh:property [
     sh:path <http://ogp.me/ns#tag>;
     sh:maxCount 16
   ] ;
   sh:property [
     sh:path <http://ogp.me/ns#title>;
     sh:minCount 1
   ] ;
   sh:property [
     sh:path <http://ogp.me/ns#title>;
     sh:maxCount 1
   ] ;
   sh:property [
     sh:path <http://purl.org/stuff/rev#hasReview>;
     sh:minCount 1
   ] ;
   sh:property [
     sh:path <http://schema.org/caption>;
     sh:minCount 1
   ] ;
   sh:property [
     sh:path <http://schema.org/caption>;
     sh:maxCount 1
   ] ;
   sh:property [
     sh:path <http://schema.org/contentRating>;
     sh:minCount 1
   ] ;
   sh:property [
     sh:path <http://schema.org/contentRating>;
     sh:maxCount 1
   ] ;
   sh:property [
     sh:path <http://schema.org/contentSize>;
     sh:minCount 1
   ] ;
   sh:property [
     sh:path <http://schema.org/contentSize>;
     sh:maxCount 1
   ] ;
   sh:property [
     sh:path <http://schema.org/keywords>;
     sh:minCount 1
   ] ;
   sh:property [
     sh:path sorg:description;
     sh:minCount 1
   ] ;
   sh:property [
     sh:path sorg:text;
     sh:maxCount 1
   ] ;
   sh:property [
     sh:path <http://xmlns.com/foaf/homepage>;
     sh:minCount 1
   ] .
