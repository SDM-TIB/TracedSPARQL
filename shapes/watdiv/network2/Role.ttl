@prefix ub: <http://db.uwaterloo.ca/~galuc/wsdbm/> .
@prefix sorg: <http://schema.org/> .
@prefix foaf: <http://xmlns.com/foaf/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix : <http://example.org/> .

:Role a sh:NodeShape;
    sh:targetClass ub:Role;
    sh:property [
        sh:path ub:follows;
        sh:minCount 1
    ] ;
    sh:property [
        sh:path ub:friendOf;
        sh:minCount 1
    ] ;
    sh:property [
        sh:path ub:gender;
        sh:minCount 1
    ] ;
    sh:property [
        sh:path ub:gender;
        sh:maxCount 1
    ] ;
    sh:property [
        sh:path ub:likes;
        sh:qualifiedValueShape [
            sh:node :ProductCategory
        ] ;
        sh:qualifiedMinCount 1
    ] ;
    sh:property [
        sh:path ub:makesPurchase;
        sh:minCount 3
    ] ;
    sh:property [
        sh:path ub:makesPurchase;
        sh:maxCount 15
    ] ;
    sh:property [
        sh:path ub:userId;
        sh:minCount 1
    ] ;
    sh:property [
        sh:path ub:userId;
        sh:maxCount 1
    ] ;
    sh:property [
        sh:path sorg:email;
        sh:minCount 1
    ] ;
    sh:property [
        sh:path sorg:jobTitle;
        sh:minCount 1
    ] ;
    sh:property [
        sh:path sorg:jobTitle;
        sh:maxCount 1
    ] ;
    sh:property [
        sh:path sorg:nationality;
        sh:minCount 1
    ] ;
    sh:property [
        sh:path sorg:nationality;
        sh:maxCount 1
    ] ;
    sh:property [
        sh:path foaf:age;
        sh:minCount 1
    ] ;
    sh:property [
        sh:path foaf:age;
        sh:maxCount 1
    ] ;
    sh:property [
        sh:path foaf:familyName;
        sh:minCount 1
    ] ;
    sh:property [
        sh:path foaf:familyName;
        sh:maxCount 1
    ] ;
    sh:property [
        sh:path foaf:givenName;
        sh:minCount 1
    ] ;
    sh:property [
        sh:path foaf:givenName;
        sh:maxCount 1
    ] ;
    sh:property [
        sh:path foaf:homepage;
        sh:minCount 1
    ] ;
    sh:property [
        sh:path foaf:homepage;
        sh:maxCount 1
    ] .
